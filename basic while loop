User_prompt = "Enter a todo:"

todos = []
while 2 > 1:
    todo = input(User_prompt)
    todo1 = input(User_prompt)
    print(todo.capitalize())
    todos.append(todo)
    todos.append(todo1)
    print(todos)

#Method two for getting output in a title format
User_prompt = "Enter a todo:"

todos = []
while 2 > 1:
    todo = input(User_prompt)
    print(todo.title())
    todos.append(todo)
    print(todos)


#
x = 1

while x <= 6:
    print(x)
    x = x + 1
